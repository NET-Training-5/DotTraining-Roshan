@model (int, int)
@{
    (int from, int to)= Model;
    }
    <h2>miltiplication table from @from to @to </h2>

  @for(int i = from; i<=to; i++){

   @for (int k = 1; k <= 10; k++)
    {

        var ans = (i * k);


        <pre>@i * @k = @ans</pre>

    }
    }

